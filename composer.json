{
    "name": "sig/api",
    "type": "project",
    "description": "API da aplicação SIG.",
    "keywords": [
        "framework",
        "laravel",
        "sgi",
        "api"
    ],
    "license": "MIT",
    "http-basic": {
        "https://github.com/lucasrodrigobento/laravel-10": {
            "username": "lucasrodrigobento@outlook.com",
            "password": "Hfn64luc@s"
        }
    },
    "require": {
        "php": "8.1.21",
        "barryvdh/laravel-dompdf": "2.0.1",
        "darkaonline/l5-swagger": "8.5.1",
        "facade/ignition": "2.17.7",
        "fideloper/proxy": "^4.0",
        "fruitcake/laravel-cors": "^3.0",
        "intervention/image": "^2.7",
        "kylekatarnls/laravel-carbon-2": "^1.0.0",
        "laravel-shift/shift-cli": "v1.0.0",
        "laravel/framework": "8.0",
        "laravel/helpers": "^1.6",
        "laravel/passport": "^11.0",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^3.0",
        "lcobucci/jwt": "^4.0",
        "nesbot/carbon": "2.67.0 as 1.39.0",
        "phpoffice/phpspreadsheet": "^1.14",
        "ramsey/uuid": "^4.7",
        "tymon/jwt-auth": "^1.0@dev",
        "guzzlehttp/guzzle": "^7.0.1"
    },
    "require-dev": {
        "laravel-shift/shift-cli": "v1.0.0",
        "beyondcode/laravel-dump-server": "^1.0",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^5.0",
        "phpunit/phpunit": "^9.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "kylekatarnls/update-helper": true
        },
        "github-oauth": {
            "github.com": "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"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/lucasrodrigobento/laravel-10.git"
        }
    ],
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "App\\Http\\Repository\\Auth\\": "app/Http/Repository/Auth/",
            "App\\Helpers\\": "app/Helpers"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}